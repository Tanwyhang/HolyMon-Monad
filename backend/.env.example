# Environment
MONAD_RPC_URL=https://testnet-rpc.monad.xyz
CHAIN_ID=14314

# Backend Wallet
PRIVATE_KEY=0x...

# Contract Addresses (will be populated after deployment)
AGENT_REGISTRY_ADDRESS=
TOKEN_LAUNCHPAD_ADDRESS=
MON_STAKING_ADDRESS=

# ElizaOS LLM Configuration
GROQ_API_KEY=gsk_xxx  # Get free key at https://console.groq.com/
ELIZAOS_MODEL_PROVIDER=groq
ELIZAOS_HYBRID_RATIO=0.5  # 0.5 = 50% AI, 50% templates (lower = cheaper)
ELIZAOS_CACHE_TTL=300000  # Cache responses for 5 minutes (ms)
ELIZAOS_LLM_TIMEOUT=10000  # LLM timeout in ms (10s default)

# Optional: OpenAI (if you prefer over Groq)
OPENAI_API_KEY=

# Server
PORT=8765
NODE_ENV=development

# Optional: API Key for requests
API_KEY=

